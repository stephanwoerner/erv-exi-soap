	<xsd:annotation>
		<xsd:documentation>EXI Types</xsd:documentation>
	</xsd:annotation>
	<!-- SIMPLE TYPES -->
	<xsd:simpleType name="addressCustomerType">
		<xsd:annotation>
			<xsd:documentation>type for address [Typ fuer Anrede]</xsd:documentation>
			<xsd:documentation>hr=mister [herr]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="hr"/>
			<xsd:enumeration value="HR"/>
			<xsd:enumeration value="fr"/>
			<xsd:enumeration value="FR"/>
			<xsd:enumeration value="fa"/>
			<xsd:enumeration value="FA"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="addressDifferingAccountHolderType">
		<xsd:annotation>
			<xsd:documentation>type for address [Typ fuer Anrede]</xsd:documentation>
			<xsd:documentation>hr=mister [herr]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="hr"/>
			<xsd:enumeration value="HR"/>
			<xsd:enumeration value="fr"/>
			<xsd:enumeration value="FR"/>
			<xsd:enumeration value="fa"/>
			<xsd:enumeration value="FA"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tariffCategoryType">
		<xsd:annotation>
			<xsd:documentation>type for tariffCategory [Typ fuer Tarif Kategorie,derzeit nur Rentals]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="AlphaStringLength2">
			<xsd:enumeration value="AC"/>
			<xsd:enumeration value="ac"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TripTypeType">
		<xsd:annotation>
			<xsd:documentation>type for tripType [Typ fuer Verkehrsmitteltyp]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="AlphaStringLength3">
			<xsd:enumeration value="BUS"/>
			<xsd:enumeration value="bus"/>
			<xsd:enumeration value="CAR"/>
			<xsd:enumeration value="car"/>
			<xsd:enumeration value="CRU"/>
			<xsd:enumeration value="cru"/>
			<xsd:enumeration value="FLT"/>
			<xsd:enumeration value="flt"/>
			<xsd:enumeration value="TRN"/>
			<xsd:enumeration value="trn"/>
			<xsd:enumeration value="SCH"/>
			<xsd:enumeration value="sch"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="addressTravellerType">
		<xsd:annotation>
			<xsd:documentation>type for address [Typ fuer Anrede]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="fr"/>
			<xsd:enumeration value="hr"/>
			<xsd:enumeration value="FR"/>
			<xsd:enumeration value="HR"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="money">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Es sind maximal 3-Dezimalstellen erlaubt.</xsd:documentation>
			<xsd:documentation xml:lang="en">Used for amounts, max 3 decimals</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="3"/>
			<xsd:minInclusive value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="currencyCodeISO4217">
		<xsd:annotation>
			<xsd:documentation xml:lang="de">Währungskennzeichen nach ISO 4217 (3 Zeichen)</xsd:documentation>
			<xsd:documentation xml:lang="en"> ISO 4217 currency code</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-zA-Z]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AlphaStringLength2">
		<xsd:annotation>
			<xsd:documentation>Language Code ISO 639-2-Alpha-2 [Sprachkennzeichen]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[A-Z]{2}|[a-z]{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="paymentMethodType">
		<xsd:annotation>
			<xsd:documentation>type for method of payment [Zahlungsweise]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="be"/>
			<xsd:enumeration value="BE"/>
			<xsd:enumeration value="ai"/>
			<xsd:enumeration value="AI"/>
			<xsd:enumeration value="kk"/>
			<xsd:enumeration value="KK"/>
			<xsd:enumeration value="kx"/>
			<xsd:enumeration value="KX"/>
			<xsd:enumeration value="ap"/>
			<xsd:enumeration value="AP"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="secureHandleIDType">
		<xsd:annotation>
			<xsd:documentation>type for ERV payment securehandle ID [Typ fuer ERV payment securehandle ID]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="128"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="payPageHandleIDType">
		<xsd:annotation>
			<xsd:documentation>type for ERV payPagehandle ID [Typ fuer ERV payPagehandle ID]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="128"/>
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="servicePremiumType">
		<xsd:annotation>
			<xsd:documentation>type for servicePremium [Typ fuer ERV Service-Prämie]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="130"/>
			<xsd:minLength value="0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StringLength1to16">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Character Strings, length 1 to 16.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StringLength1to6">
