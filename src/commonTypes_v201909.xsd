	</xsd:simpleType>
	<xsd:simpleType name="StringLength0to60">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Character Strings, length 0 to 60.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"/>
			<xsd:maxLength value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StringLength1to60">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Character Strings, length 0 to 60.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StringLength0to50">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Character Strings, length 0 to 50</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="0"/>
			<xsd:maxLength value="50"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="StringLength0to100">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Used for Character Strings, length 0 to 100</xsd:documentation>
